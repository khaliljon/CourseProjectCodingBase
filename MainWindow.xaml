<Window x:Class="CourseProjectCodingBase.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Курсовой проект Каландарова Халила" Height="600" Width="850">
    <Grid>
        <DockPanel>
            <StackPanel DockPanel.Dock="Left" Width="200" Background="LightGray">
                <TextBlock Text="Лабораторные работы" FontSize="16" FontWeight="Bold" Margin="10"/>
                <Button Content="Лабораторная 2" Click="OpenLab2" Margin="5"/>
                <Button Content="Лабораторная 3" Click="OpenLab3" Margin="5"/>
                <Button Content="Лабораторная 4" Click="OpenLab4" Margin="5"/>
                <!-- Добавлена кнопка для Лабораторной работы 4 -->
            </StackPanel>

            <Frame Name="MainFrame" NavigationUIVisibility="Hidden" />
        </DockPanel>
    </Grid>
</Window>